{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src"],
			"@/assets/*": ["./assets/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
	"exclude": ["node_modules"]
}
